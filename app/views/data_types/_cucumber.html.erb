<div class="row">
  <div class=col-sm-9>
    <div class="row steps-header" >
      <div class="col-sm-1">#</div>
      <div class="col-sm-4">Step</div>
      <div class="col-sm-2">Status</div>
      <div class="col-sm-4">Exception</div>
      <div class="col-sm-1">Embeds</div>
    </div>
    
    <% data['steps'].each do |id , step| %>
    <div class="row step" id="step_<%= id %>" >
      <div class="col-sm-1">
        <%= id %>
      </div>
      <div class="col-sm-4">
        <b><%= step['keyword'] %></b> <%= step['step_name'] %>
      </div>
      <div class="col-sm-2">
        <span class="tag tag-<%= step['status'] == "passed" ? "success" : "danger" %>"><%= step['status'].upcase  %></span>
      </div>
      <div class="col-sm-4">
        <%= step['exception']  %>
      </div>
      <div class="col-sm-1">
        <% if !step['embeddings'].nil? %>
           <% step['embeddings'].each do |key,embedding| %>
               <img onclick='LoadModalWithImg(this.src)' height=20 width=20 src="<%= embedding['src'] %>"/>
           <% end %>
        <% end %>
      </div>
    </div>
    <% end %>
  </div>
  <div class=col-sm-3>
    step desc
  </div>
</div>
