


<div class="container">
<%= form_for(resource, as: resource_name, url: session_path(resource_name), :html => { :class => "form-signin" }) do |f| %>
      <%= devise_error_messages! %>
        <h2 class="form-signin-heading">Please sign in</h2>
        <label for="inputEmail" class="sr-only">Email address</label>
        <%= f.email_field :email, autofocus: true , :class => "form-control first"%>
        <label for="inputPassword" class="sr-only">Password</label>
        <%= f.password_field :password, autocomplete: "off", :class => "form-control last" %>
        <div class="checkbox">
          <label>
            <%= f.check_box :remember_me %> Remember me
          </label>
        </div>
        <%= f.submit "Log in", :class => "btn btn-lg btn-primary btn-block" %>
        <%= link_to "Sign up", new_registration_path(resource_name), :class => "btn btn-lg btn-primary btn-block"  %>
        <%= link_to "Forgot your password?", new_password_path(resource_name) %>
<% end %>
</div>