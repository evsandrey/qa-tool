<div class="container m-t-1 m-b-1">
  <%= link_to examples_path, class: 'btn btn-outline-primary pull-md-right m-l-1' do %>
    Back
  <% end %>
  <%= link_to edit_example_path(@example), class: 'btn btn-primary pull-md-right m-l-1' do %>
    Edit
  <% end %>
  <h1>Show example</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @example.name %></dd>

  <dt>Description:</dt>
  <dd><%= @example.description %></dd>

  <dt>Bool:</dt>
  <dd><%= @example.bool %></dd>

</dl>
