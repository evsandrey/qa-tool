<div class="container m-t-1 m-b-1">
  <%= link_to logs_path, class: 'btn btn-outline-primary pull-md-right m-l-1' do %>
    Back
  <% end %>
  <%= link_to edit_log_path(@log), class: 'btn btn-primary pull-md-right' do %>
    Edit
  <% end %>
  <h1>Show log</h1>
</div>

<dl class="dl-horizontal">
  <dt>Module:</dt>
  <dd><%= @log.module_name %></dd>

  <dt>Level:</dt>
  <dd><%= @log.level %></dd>

  <dt>Message:</dt>
  <dd><%= @log.message %></dd>

</dl>
