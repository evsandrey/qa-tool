<div class="container m-t-1 m-b-1">
  <%= link_to product_version_suites_path(@product,@version), class: 'btn btn-outline-primary pull-md-right m-l-1' do %>
    Back
  <% end %>
  <%= link_to edit_product_version_suite_path(@product,@version,@suite), class: 'btn btn-info pull-md-right' do %>
    Edit
  <% end %>
  <h1>Show suite</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @suite.name %></dd>

  <dt>Description:</dt>
  <dd><%= @suite.description %></dd>

  <dt>Custom params:</dt>
    <%= render :partial => 'shared/custom_params', :locals => {:custom_params => @suite.custom_params} %>
</dl>
