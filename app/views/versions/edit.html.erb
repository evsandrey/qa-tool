<div class="container m-t-1 m-b-1">
  <%= link_to product_versions_path(@product), class: 'btn btn-outline-primary pull-md-right m-l-1' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to product_version_path(@product,@version), class: 'btn btn-primary pull-md-right  m-l-1' do %>
